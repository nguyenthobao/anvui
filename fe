--
-- Table structure for table `web`
--

DROP TABLE IF EXISTS `web`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `web` (
  `idw` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `tokenKey` text,
  `userId` varchar(255) DEFAULT NULL,
  `anvui_id` varchar(255) DEFAULT NULL,
  `home_anvui` tinyint(4) unsigned DEFAULT '0' COMMENT '1.web đang hoạt động, 2.website dùng thử, 3 website tự khoá',
  `idcat` smallint(6) unsigned DEFAULT '1',
  `w_name` varchar(255) DEFAULT NULL,
  `s_name` varchar(60) NOT NULL,
  `d_web` varchar(255) DEFAULT NULL,
  `id_anvui` varchar(255) DEFAULT NULL,
  `uid` int(11) unsigned DEFAULT NULL,
  `email_web` varchar(60) DEFAULT NULL,
  `email_user` varchar(60) DEFAULT NULL,
  `address` varchar(255) DEFAULT NULL,
  `city_id` smallint(6) DEFAULT NULL,
  `distric_id` smallint(6) DEFAULT NULL,
  `country_id` smallint(6) DEFAULT NULL,
  `phone` varchar(16) DEFAULT NULL,
  `start_date` int(11) unsigned NOT NULL,
  `end_date` int(11) unsigned NOT NULL DEFAULT '0',
  `blocking` tinyint(4) unsigned DEFAULT '0',
  `hidden` tinyint(4) unsigned DEFAULT '0',
  `db_id` tinyint(4) unsigned DEFAULT '1',
  `vip` tinyint(11) unsigned DEFAULT '0',
  `domain` text,
  `support_id` int(11) unsigned DEFAULT NULL,
  `sale_id` int(11) unsigned DEFAULT NULL,
  `contract_id` int(11) unsigned DEFAULT NULL,
  `view` bigint(20) unsigned DEFAULT '0',
  `theme_id` bigint(20) unsigned DEFAULT '1',
  `theme_mobi_id` smallint(6) unsigned DEFAULT '1',
  `saleoff_id` int(11) unsigned DEFAULT NULL,
  `transaction_sum` int(11) unsigned DEFAULT '0',
  `status` tinyint(4) unsigned DEFAULT '1' COMMENT '1.web đang hoạt động, 2.website dùng thử, 3 website tự khoá',
  `mod_use` varchar(50) DEFAULT NULL,
  `closeweb` text,
  `giahan` tinyint(4) NOT NULL DEFAULT '0',
  `idw_v1` bigint(20) DEFAULT NULL,
  `using` tinyint(1) DEFAULT '0',
  `kdcare` varchar(255) DEFAULT NULL,
  `note` varchar(255) DEFAULT NULL,
  `key_api` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`idw`),
  UNIQUE KEY `index` (`idw`,`s_name`) USING BTREE,
  KEY `index1` (`idcat`,`uid`,`status`)
) ENGINE=InnoDB AUTO_INCREMENT=3219 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

