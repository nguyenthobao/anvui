--
-- Table structure for table `user`
--

DROP TABLE IF EXISTS `user`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `user` (
  `uid` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `first_name` varchar(20) DEFAULT NULL,
  `last_name` varchar(20) DEFAULT NULL,
  `full_name` varchar(80) DEFAULT NULL,
  `username` varchar(30) DEFAULT '',
  `password` varchar(50) NOT NULL,
  `email` varchar(60) NOT NULL,
  `phone` varchar(16) DEFAULT '',
  `address` varchar(255) DEFAULT NULL,
  `distric` varchar(50) DEFAULT NULL,
  `city` varchar(50) DEFAULT NULL,
  `country` varchar(50) DEFAULT NULL,
  `distric_id` smallint(5) unsigned DEFAULT NULL,
  `city_id` smallint(5) unsigned DEFAULT NULL,
  `country_id` tinyint(3) unsigned DEFAULT NULL,
  `birthday` int(10) unsigned DEFAULT NULL,
  `avatar` varchar(255) DEFAULT NULL,
  `create_time` int(10) unsigned DEFAULT NULL,
  `last_login` int(11) DEFAULT NULL,
  `skype` varchar(30) DEFAULT NULL,
  `yahoo` varchar(30) DEFAULT NULL,
  `facebook_id` bigint(20) unsigned DEFAULT NULL,
  `google_id` bigint(20) unsigned DEFAULT NULL,
  `vatgia_id` bigint(20) unsigned DEFAULT NULL,
  `about_info` text,
  `is_admin` tinyint(3) unsigned DEFAULT '0',
  `is_admin_web` varchar(255) DEFAULT NULL,
  `status` tinyint(3) unsigned DEFAULT '0',
  `create_ip` varchar(255) DEFAULT NULL,
  `code` mediumint(1) DEFAULT '0',
  `plus_id` varchar(512) DEFAULT NULL,
  PRIMARY KEY (`uid`),
  UNIQUE KEY `index` (`uid`,`username`,`email`,`facebook_id`,`google_id`,`vatgia_id`) USING BTREE,
  KEY `index1` (`distric_id`,`city_id`,`country_id`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

