--
-- Table structure for table `themes_category`
--

DROP TABLE IF EXISTS `themes_category`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `themes_category` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `parent_id` int(11) DEFAULT NULL,
  `title` varchar(100) DEFAULT NULL,
  `description` text,
  `img` varchar(255) DEFAULT NULL,
  `icon` varchar(255) DEFAULT NULL,
  `is_home` tinyint(4) DEFAULT NULL,
  `meta_keyword` varchar(255) DEFAULT NULL,
  `meta_description` text,
  `create_uid` bigint(11) DEFAULT NULL,
  `update_uid` bigint(11) DEFAULT NULL,
  `create_time` int(11) DEFAULT NULL,
  `update_time` int(11) DEFAULT NULL,
  `bg` varchar(255) DEFAULT NULL,
  `status` tinyint(4) DEFAULT '1',
  `sort` smallint(6) DEFAULT NULL,
  `meta_title` varchar(100) DEFAULT NULL,
  `number_home` smallint(6) DEFAULT NULL,
  `alias` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `index` (`parent_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

