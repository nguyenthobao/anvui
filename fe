  `permission` varchar(255) DEFAULT NULL,
  `point` int(11) DEFAULT '0',
  PRIMARY KEY (`uid`),
  UNIQUE KEY `index` (`uid`,`username`,`email`,`facebook_id`,`google_id`,`vatgia_id`) USING BTREE,
  KEY `index1` (`distric_id`,`city_id`,`country_id`) USING BTREE,
